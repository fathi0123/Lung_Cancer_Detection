                            <input class="form-control" type="text" value="@item.CHEST_PAIN" id="CHEST_PAIN" name="CHEST_PAIN">
                        </div>
                        <div class="mb-3">
                            <label for="SHORTNESS_OF_BREATH" class="form-label">
                                ضيق في التنفس   (SHORTNESS OF BREATH)
                            </label>
                            <input class="form-control" type="text" value="@item.SWALLOWING_DIFFICULTY" id="SHORTNESS_OF_BREATH" name="SHORTNESS_OF_BREATH">
                        </div>
                        <div class="mb-3">
                            <label for="SHORTNESS_OF_BREATH" class="form-label">
                                الإجمالي    (Total Degree)
                            </label>
                            <input class="form-control" type="text" value="@item.Total_Degree" id="Total_Degree" name="Total_Degree">
                        </div>

                    </div>
                </div>

                <div class="col-lg-6">
                    <div class="mb-3">
                        <label for="YELLOW_FINGERS" class="form-label">الاصابع الصفراء (YELLOW_FINGERS) </label>
                        <input class="form-control" type="text" value="@item.YELLOW_FINGERS" id="YELLOW_FINGERS" name="YELLOW_FINGERS">
                    </div>
                    <div class="mb-3">
                        <label for="SMOKING" class="form-label">نسبة التدخين (SMOKING)  </label>
                        <input class="form-control" type="text" value="@item.SMOKING" id="SMOKING" name="SMOKING">
                    </div>
                    <div class="mb-3">
                        <label for="ANXIETY" class="form-label">قلق  (ANXIETY)  </label>
                        <input class="form-control" type="text" value="@item.ANXIETY" id="ANXIETY" name="ANXIETY">
                    </div>
                    <div class="mb-3">
